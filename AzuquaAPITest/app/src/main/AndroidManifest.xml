<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sasidhar.azuqua.com.azuquaapitest" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".AzuquaApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".APIConfigurationsListActivity"
            android:label="@string/title_activity_apiconfigurations_list" >
        </activity>
        <activity
            android:name=".DashBoardActivity"
            android:label="@string/title_activity_dash_board" >
        </activity>
        <activity
            android:name=".FloActivity"
            android:label="@string/title_activity_flo" >
        </activity>
        <activity
            android:name=".ResultActivity"
            android:label="@string/title_activity_result" >
        </activity>
        <activity
            android:name=".NewAPIActivity"
            android:label="@string/title_activity_new_api" >
        </activity>
    </application>

</manifest>
